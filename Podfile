target 'FourSquareApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod ‘Alamofire’, ‘~> 4.0’

  # Pods for FourSquareApp

  target 'FourSquareAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FourSquareAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
